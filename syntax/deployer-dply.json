{
  "name": "deployer-dply",
  "scopeName": "source.deployer-dply",
  "patterns": [
    {
      "include": "#literate"
    },
    {
      "include": "#target"
    }
  ],
  "repository": {
    "literate": {
      "name": "comment.block",
      "match": "^[^ \t].*$"
    },
    "target": {
      "name": "keyword.target",
      "match": "target"
    }
  }
}
